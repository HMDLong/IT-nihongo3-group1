<br>
<h1>Your info</h1>
<br>
<table class="table table-hover">
  <tbody>
    <tr class="table-active">
      <th scope="row">Name</th>
      <td><%= @user.name %></td>
    </tr>
    <tr>
      <th scope="row">Phone Number</th>
      <td><%= @user.phone %></td>
    </tr>
    <tr class="table-primary">
      <th scope="row">Address</th>
      <td><%= @user.address %></td>
    </tr>
  </tbody>
</table>
<br>
<%= link_to "Edit info", edit_user_registration_path, class: "btn btn-outline-primary" %>
